  <rosparam file="$(find height_mapping)/launch/config/params.yaml" command="load" subst_value="true" />

  <!-- Height Mapping -->
  <node pkg="height_mapping" type="height_mapping_node" name="height_mapping" output="screen" />

  <!-- Height Map Visualization -->
  <node pkg="height_mapping" type="height_map_visualization_node" name="height_map_visualization" output="screen" />

</launch>